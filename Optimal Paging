#include <stdio.h>

int main() {
    int pages[] = {1, 2, 3, 4, 1, 2, 5, 1, 2, 3, 4, 5};
    int n = 12;   
    int f = 3;    
    int frame[3];
    int i, j, k, faults = 0, flag, replace, farthest, pos;

    for (i = 0; i < f; i++)
        frame[i] = -1;

    printf("Page\tFrames\t\tPage Fault\n");

    for (i = 0; i < n; i++) {
        flag = 0;

        // check if page already in frame
        for (j = 0; j < f; j++) {
            if (frame[j] == pages[i]) {
                flag = 1;  
                break;
            }
        }

        if (flag == 0) { // page fault
            // if empty slot available
            for (j = 0; j < f; j++) {
                if (frame[j] == -1) {
                    frame[j] = pages[i];
                    faults++;
                    flag = 2;
                    break;
                }
            }

            if (flag == 0) { // need replacement
                farthest = i + 1;
                replace = -1;

                for (j = 0; j < f; j++) {
                    pos = -1;
                    for (k = i + 1; k < n; k++) {
                        if (frame[j] == pages[k]) {
                            pos = k;
                            break;
                        }
                    }
                    if (pos == -1) { 
                        replace = j;
                        break;
                    }
                    if (pos > farthest) {
                        farthest = pos;
                        replace = j;
                    }
                }

                frame[replace] = pages[i];
                faults++;
            }

            printf("%d\t", pages[i]);
            for (j = 0; j < f; j++) {
                if (frame[j] != -1)
                    printf("%d ", frame[j]);
                else
                    printf("- ");
            }
            printf("\tYes\n");
        } else {
            printf("%d\t", pages[i]);
            for (j = 0; j < f; j++) {
                if (frame[j] != -1)
                    printf("%d ", frame[j]);
                else
                    printf("- ");
            }
            printf("\tNo\n");
        }
    }

    printf("\nTotal Page Faults = %d\n", faults);
    return 0;
}

output

Page    Frames          Page Fault
1       1 - -           Yes
2       1 2 -           Yes
3       1 2 3           Yes
4       4 2 3           Yes
1       4 2 3           No
2       4 2 3           No
5       5 2 3           Yes
1       1 2 3           Yes
2       1 2 3           No
3       1 2 3           No
4       4 2 3           Yes
5       5 2 3           Yes

Total Page Faults = 7
